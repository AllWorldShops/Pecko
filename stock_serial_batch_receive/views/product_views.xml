<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="product_category_form_view_inherit"
		model="ir.ui.view">
		<field name="name">product.category.form</field>
		<field name="model">product.category</field>
		<field name="inherit_id" ref="account.view_category_property_form"/>
		<field name="arch" type="xml">
			<xpath expr="//group[@name='account_property']" position="after">
				<group name="sequence">
					<group string="Product Sequence Local">
						<field name="product_sequence_id"/>
					</group>
					<group string="Product Sequence Overseas">
						<field name="product_sequence_overseas_id"/>
					</group>
				</group>
			</xpath>
		</field>
	</record>
</odoo>